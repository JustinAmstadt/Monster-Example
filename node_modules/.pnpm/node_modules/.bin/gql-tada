#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules/gql.tada/bin/node_modules:/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules/gql.tada/node_modules:/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules:/home/justin/sui/monster/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules/gql.tada/bin/node_modules:/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules/gql.tada/node_modules:/home/justin/sui/monster/node_modules/.pnpm/gql.tada@1.4.3_graphql@16.8.1_typescript@5.4.5/node_modules:/home/justin/sui/monster/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gql.tada/bin/cli.js" "$@"
else
  exec node  "$basedir/../gql.tada/bin/cli.js" "$@"
fi
